{
  "name": "webpacktemplate",
  "version": "5.0.0",
  "description": "VanillaJS webpack template",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --open --config webpack.dev.conf.js",
    "stage": "webpack --mode none --config webpack.stage.conf.js",
    "prod": "webpack --config webpack.prod.conf.js",
    "eslint": "eslint src/components/",
    "eslint--fix": "eslint src/components/ --fix",
    "stylelint": "npx stylelint src/components/*/*.scss",
    "stylelint--fix": "npx stylelint src/components/*/*.scss --fix"
  },
  "browserslist": [
    "> 1%",
    "last 3 version"
  ],
  "author": "Yastrebtsov",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-airbnb": "^4.4.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.0",
    "stylelint-order": "^4.0.0",
    "svg-sprite-loader": "^4.2.1",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "choices.js": "^9.0.1",
    "croppie": "^2.6.5",
    "formbouncerjs": "^1.4.6",
    "inputmask": "^5.0.3",
    "photoswipe": "^4.1.3",
    "swiper": "^5.3.1"
  }
}
